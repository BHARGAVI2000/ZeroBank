package javaBasics;

public class BasicPrograms {

	public static void main(String[] args) {
		add();
		// TODO Auto-generated method stub
		
	}
	
		public static void add() {
		int a=10;
		int b=20;
		int c =a+b;
		System.out.println(c);// the problem with this program is always we add the same two values
		
		 //and again we can't reuse the same variable in the same method or main method and add another two values so to avoid this we use methods
			
	}
	
	
	

}
