package NewToursDemoAut;

import java.util.concurrent.TimeUnit;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.firefox.FirefoxDriver;

public class usernamePassword {

	public static void main(String[] args) throws InterruptedException {
		System.setProperty("webdriver.gecko.driver", "C:\\drivers\\geckodriver.exe");
		WebDriver driver = new FirefoxDriver();
		driver.manage().window().maximize();    
		driver.manage().timeouts().implicitlyWait(20, TimeUnit.SECONDS);
		
		
	driver.get("http://ww1.demoaut.com/");
		
	
		
		Thread.sleep(3000);
		
		driver.findElement(By.xpath("//body")).click();
		Thread.sleep(3000);
	
	driver.findElement(By.xpath("//body")).click();
		
		
	
	//driver.findElement(By.xpath("//*[@id=\"Contentplaceholder1_TB620CF35028_Col01\"]/div/div/div/a[2]")).click();
	
//System.out.println(	driver.findElement(By.xpath("/html/body/div[2]/div[1]/form/div[1]/div/label")).getAttribute("name"));
	
	
//System.out.println(	driver.findElement(By.xpath("/html/body/div[2]/div[1]/form/div[2]/label")).getAttribute("name"));

	
	
	//System.out.println(driver.findElement(By.xpath("//*[@id=\"Contentplaceholder1_TB620CF35028_Col01\"]/div/div/div/a[2]")).getText());
	}

}
